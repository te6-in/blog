---
import { $path } from "astro-typesafe-routes/path";
import { Rss as RssIcon } from "lucide-astro";
import Base from "./Base.astro";

interface Props {
  class?: string;
}
---

<Base
  title="RSS 피드"
  aria-label="RSS 피드"
  class={Astro.props.class}
  href={$path({ to: "/rss.xml" })}
>
  <RssIcon size={15} class="flex-none" stroke-width={3} />
</Base>
